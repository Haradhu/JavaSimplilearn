package com.har.corejava;



abstract class InnerClassAnonymous{  
	  abstract void come();  
	}  
	class Innerclass{  
	 public static void main(String args[])
	 {  
		 InnerClassAnonymous n=new InnerClassAnonymous()
		 {  
	  void come()
	  {
		  System.out.println("Welcome Home");
		  }  
	  };
	  n.come();  
	 }  
	}  

